<%= form_for address do |f| %>
    <div class="form-group">
		<%= f.label :street_address %>
		<%= f.text_field :street_address %>
	</div>

	<div class="form-group">
		<%= f.label :city_id %>
		<%= f.collection_select(:city_id, City.all, :id, :name) %>
    </div>

	<div class="form-group">
		<%= f.label :state_id %>
		<%= f.collection_select(:state_id, State.all, :id, :name) %>
    </div>

	<div class="form-group">
		<%= f.submit %>
	</div>
<% end %>
